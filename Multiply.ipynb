    "def multiply(a, b):\n",
    "    count = 0\n",
    "    alll = []\n",
    "    while count < b.shape[1]:\n",
    "        col = list(b.iloc[:,count])\n",
    "        dots = []\n",
    "        count2 = 0\n",
    "        while count2 < a.shape[0]:\n",
    "            row = list(a.loc[count2])\n",
    "            dot = 0\n",
    "            count3 = 0\n",
    "            while count3 < len(row):\n",
    "                dot += row[count3]*col[count3]\n",
    "                count3 += 1\n",
    "            dots.append(dot)\n",
    "            count2 += 1\n",
    "        alll.append(dots)\n",
    "        count += 1\n",
    "    ab = pd.DataFrame()\n",
    "    i = 0\n",
    "    col_count = list(b.columns)\n",
    "    while i < len(col_count):\n",
    "        ab['Col ' + str(i+1)] = alll[i]\n",
    "        i += 1\n",
    "    return ab\n",
    "    "
